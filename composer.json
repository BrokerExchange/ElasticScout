{
  "name": "brokerexchange/elasticscout",
  "description": "Laravel Scout Driver for Elasticsearch 5.x",
  "keywords": [
    "elasticsearch",
    "search",
    "laravel",
    "scout"
  ],
  "license": "MIT",
  "homepage": "http://brokerexchange.github.io/ElasticScout/",
  "authors": [
    {
      "name": "Brian Mix",
      "email": "brian@brokerbin.com"
    },
    {
      "name": "Jen Page",
      "email": "jpage@brokerbin.com"
    }
  ],
  "minimum-stability": "stable",
  "suggests": {
    "elasticsearch/elasticsearch": "Official Elasticsearch Client",
    "laravel/scout": "Laravel Official Fulltext Search Package"
  },
  "require-dev": {
    "monolog/monolog": "^1.23"
  },
  "autoload": {
    "psr-4": {
      "ElasticScout\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "ElasticScout\\ElasticScoutServiceProvider"
      ],
      "aliases": {
        "Agg": "ElasticScout\\Facades\\Agg",
        "DSL": "ElasticScout\\Fadades\\DSL"
      }
    }
  }
}
