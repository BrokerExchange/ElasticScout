{
  "name": "brokerexchange/elasticscout",
  "description": "Laravel Scout Driver for Elasticsearch 6.x",
  "keywords": [
    "elasticsearch",
    "search",
    "laravel",
    "scout"
  ],
  "license": "MIT",
  "homepage": "http://brokerexchange.github.io/ElasticScout/",
  "authors": [
    {
      "name": "Brian Mix",
      "email": "brian@brokerbin.com"
    },
    {
      "name": "Jen Page",
      "email": "jpage@brokerbin.com"
    },
    {
      "name": "Charles Peterson",
      "email": "cpeterson@brokerbin.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "elasticsearch/elasticsearch": "^6.0.1",
    "laravel/scout": "^4.0"
  },
  "autoload": {
    "psr-4": {
      "ElasticScout\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "8.0.x-dev"
    },
    "laravel": {
      "providers": [
        "ElasticScout\\ElasticScoutServiceProvider"
      ],
      "aliases": {
        "Agg": "ElasticScout\\Facades\\Agg",
        "DSL": "ElasticScout\\Facades\\DSL"
      }
    }
  }
}
