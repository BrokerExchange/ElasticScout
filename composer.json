{
  "name": "brokerexchange/elasticscout",
  "description": "Laravel Scout Driver for Elasticsearch 5.x",
  "keywords": [
    "elasticsearch",
    "search",
    "laravel",
    "scout"
  ],
  "license": "MIT",
  "homepage": "http://brokerexchange.github.io/ElasticScout/",
  "authors": [
    {
      "name": "Brian Mix",
      "email": "brian@brokerbin.com"
    },
    {
      "name": "Jen Page",
      "email": "jpage@brokerbin.com"
    },
    {
      "name": "Charles Peterson",
      "email": "cpeterson@brokerbin.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "elasticsearch/elasticsearch": "^5.3",
    "laravel/scout": "^3.0"
  },
  "require-dev": {
    "monolog/monolog": "^1.23"
  },
  "autoload": {
    "psr-4": {
      "ElasticScout\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "6.0.x-dev"
    },
    "laravel": {
      "providers": [
        "ElasticScout\\ElasticScoutServiceProvider"
      ],
      "aliases": {
        "Agg": "ElasticScout\\Facades\\Agg",
        "DSL": "ElasticScout\\Fadades\\DSL"
      }
    }
  }
}
